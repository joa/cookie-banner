{
  "manifest_version": 2,

  "name": "Cookie Banner",
  "description": "Cookie Banner Bans Cookie Banners",
  "version": "0.0.0.2",

  "author": "Joa Ebert",
  "homepage_url": "https://github.com/joa/cookie-banner",

  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "css": ["cookie-banner.css"],
    "js": ["cookie-banner.js"],
    "run_at": "document_end"
  }]
}
